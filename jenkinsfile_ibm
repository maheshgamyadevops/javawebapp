pipeline
{
	agent 
	{
		label 'maven'
	}
	stages
	{
		stage ('checkout')
		{
			steps 
			{
				git branch: 'main', credentialsId: '9ab5100a-763a-4d1b-9d43-12acbdab1f06', url: 'https://github.com/maheshgamyadevops/javawebapp.git'
			}
		}
		stage ('Build')
		{
			steps
			{
				sh "mvn clean package"
			}
		}
	}
}




